Options +FollowSymLinks
RewriteEngine On

RewriteCond %{SCRIPT_FILENAME} !-d
RewriteCond %{SCRIPT_FILENAME} !-f

# Different actions for each page type

# Stop access to protected directories
Redirect /config /home

# Catch all rule for anything that is not caught
RewriteRule ^([A-Za-z0-9\-\/]*)/?$			./index.php?id=$1				[QSA,L]